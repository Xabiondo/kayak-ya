<odoo>
    <record model="ir.module.category" id="module_category_kayak">
        <field name = "name"> Gestión de Kayaks </field>
        <field name="description "> Niveles de acceso para la aplicación Kayak ya</field>
        <field name = "sequence"> 10 </field>

    </record>
    <record id="group_kayak_user" model="res.groups">
        <field name="name"> Usuario </field>
        <field name="category_id" ref="module_category_kayak"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
    <record id="group_kayak_manager" model="res.groups">
        <field name="name">Administrador</field>
        <field name="category_id" ref="module_category_kayak"/>
        <field name="implied_ids" eval="[(4, ref('group_kayak_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
    <record id="rule_kayak_booking_personal" model="ir.rule">
        <field name="name">Ver solo mis reservas</field>
        <field name="model_id" ref="model_kayak_booking"></field>
        <field name="domain_force">[('partner_id.user_id', '=', user.id)]</field>
        <field name="groups" eval="[(4, ref('group_kayak_user'))]"></field>
    </record>

    <record id="rule_kayak_booking_manager" model="ir.rule">
        <field name="name">Administrador ve todo</field>
        <field name="model_id" ref="model_kayak_booking"></field>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('group_kayak_manager'))]"></field>


    </record>
</odoo>